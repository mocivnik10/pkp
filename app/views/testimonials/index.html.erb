<h1>Testimonials</h1>

<table>
  <thead>
    <tr>
      <th>First name</th>
      <th>Last name</th>
      <th>Company name</th>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @testimonials.each do |testimonial| %>
      <tr>
        <td><%= testimonial.first_name %></td>
        <td><%= testimonial.last_name %></td>
        <td><%= testimonial.company_name %></td>
        <td><%= testimonial.description %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
