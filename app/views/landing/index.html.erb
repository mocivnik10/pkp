								<div class="fh5co-post wow fadeInLeft"  data-wow-duration="1s" data-wow-delay="1.1s">
									<div class="fh5co-post-image">
										<div class="fh5co-overlay"></div>
										<div class="fh5co-category"><%= link_to 'Preberi Prispevek', article_path(article) %></div>
										<% if article.image? %>
						          <%= image_tag article.image.url(:medium), class:'img-article', alt:'Image' %>
						        <% else %>
											<img src="assets/img_same_dimension_2.jpg" alt="Image" class="img-article">
						        <% end %>
									</div>
									<div class="fh5co-post-text">
										<h3><%= link_to article.title, article_path(article) %></h3>
										<p><%= article.description.truncate_words(5, omission: '...').html_safe %></p>
									</div>
									<div class="fh5co-post-meta">
										<!-- <a href="#"><i class="icon-chat"></i> 33</a> -->
										<a href="#"><i class="fa fa-clock-o"></i><%= article.created_at.to_formatted_s(:short) %></a>
									</div>
								</div>
							</div>
						<% end %>
